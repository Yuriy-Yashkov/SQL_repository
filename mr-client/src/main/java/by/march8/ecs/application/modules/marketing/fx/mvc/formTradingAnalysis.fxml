<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="604.0" prefWidth="713.0"
      xmlns="http://javafx.com/javafx/9.0.1"
      fx:controller="by.march8.ecs.application.modules.marketing.fx.mvc.TradingAnalysisController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="604.0" prefWidth="910.0" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="itemName" layoutX="25.0" layoutY="3.0" prefHeight="24.0" prefWidth="775.0" text="Label"
                       AnchorPane.leftAnchor="25.0">
                    <font>
                        <Font name="DejaVu Sans Bold" size="12.0"/>
                    </font>
                </Label>
                <Label fx:id="articleNumber" layoutX="25.0" layoutY="20.0" prefHeight="24.0" prefWidth="775.0"
                       text="Label">
                    <font>
                        <Font name="DejaVu Sans Bold" size="12.0"/>
                    </font>
                </Label>
                <Button fx:id="btnProperties" layoutX="507.0" layoutY="558.0" mnemonicParsing="false"
                        onMouseClicked="#onClick" prefHeight="32.0" prefWidth="130.0" text="Параметры" visible="false"
                        AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="163.0">
                    <font>
                        <Font size="15.0"/>
                    </font>
                </Button>
                <Button fx:id="btnUpdate" layoutX="649.0" layoutY="558.0" mnemonicParsing="false"
                        onMouseClicked="#onButtonUpdateClick" prefHeight="32.0" prefWidth="130.0" text="Обновить"
                        AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Button>
                <Label fx:id="incommingAmount" layoutX="25.0" layoutY="44.0" prefHeight="24.0" prefWidth="767.0"
                       text="Label" textFill="#f3622d">
                    <font>
                        <Font name="DejaVu Sans Bold" size="12.0"/>
                    </font>
                </Label>
                <Label fx:id="outcommingAmount" layoutX="25.0" layoutY="60.0" prefHeight="24.0" prefWidth="767.0"
                       text="Label" textFill="#fba71b">
                    <font>
                        <Font name="DejaVu Sans Bold" size="12.0"/>
                    </font>
                </Label>
                <TabPane layoutX="7.0" layoutY="89.0" prefHeight="464.0" prefWidth="780.0"
                         tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="51.0" AnchorPane.leftAnchor="10.0"
                         AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="89.0">
                    <tabs>
                        <Tab text="Движение изделия">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                        <LineChart fx:id="chartMove" layoutX="11.0" layoutY="17.0" prefHeight="370.0"
                                                   prefWidth="758.0" AnchorPane.bottomAnchor="10.0"
                                                   AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                                                   AnchorPane.topAnchor="10.0">
                                            <xAxis>
                                                <CategoryAxis fx:id="categoryAxis" side="BOTTOM"/>
                                            </xAxis>
                                            <yAxis>
                                                <NumberAxis fx:id="numberAxis" side="LEFT"/>
                                            </yAxis>
                                        </LineChart>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab text="План производства">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                        <LineChart fx:id="chartPlan" layoutX="14.0" layoutY="14.0" prefHeight="370.0"
                                                   prefWidth="758.0" AnchorPane.bottomAnchor="10.0"
                                                   AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                                                   AnchorPane.topAnchor="10.0">
                                            <xAxis>
                                                <CategoryAxis fx:id="productionDateAxis" side="BOTTOM"/>
                                            </xAxis>
                                            <yAxis>
                                                <NumberAxis fx:id="productionAmountAxis" side="LEFT"/>
                                            </yAxis>
                                        </LineChart>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </AnchorPane>
    </children>
</VBox>
